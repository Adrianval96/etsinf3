        RowBox[{"[", "5", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"f", "=", 
       RowBox[{"afd", "[", 
        RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Insertar", " ", "transiciones", " ", 
         RowBox[{"{", 
          RowBox[{"q", ",", 
           RowBox[{"q", "'"}], ",", 
           RowBox[{"q", "'"}]}], "}"}], " ", 
         RowBox[{"\[ForAll]", " ", "q"}]}], ",", 
        RowBox[{
         RowBox[{"q", "'"}], " ", "\[Element]", " ", "Q"}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Insertar", " ", "transiciones", " ", 
         RowBox[{"{", 
