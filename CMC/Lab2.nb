           RowBox[{"a", "'"}]}], "}"}], " ", 
         RowBox[{"\[ForAll]", " ", "a"}]}], ",", 
        RowBox[{
         RowBox[{"a", "'"}], " ", "\[Element]", " ", "\[Sum]"}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"c", "=", "1"}], ",", 
        RowBox[{"c", "\[LessEqual]", "2"}], ",", 
        RowBox[{"c", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"For", "[", 
          RowBox[{
